################################# Задание 1 #################################

n = int(input())
a = [0]*n
lenght = 1
b = []
c = []

print("Введите массив А.")
for i in range(n):
    a[i] = int(input())

a.append("что-то, чему точно не равен последний элемент")

for i in range(n):
    if a[i] == a[i+1]:
        lenght += 1
    else:
        b.append(lenght)
        c.append(a[i])
        lenght = 1
       
print(b, c)
